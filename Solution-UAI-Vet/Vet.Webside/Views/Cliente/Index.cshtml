
@{
    ViewBag.Title = "Index";
}



<h2>Client List</h2>
<table class="table">
    <tr>
        <th>Nº</th>
        <th>Nombre</th>
        <th>Email</th>
    </tr>


    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Nombre</td>
            <td>@item.Email</td>
            <td>@Html.ActionLink("Delete", "Delete", new { id = item.Id })</td>
            <td>@Html.ActionLink("Edit", "Update", new { id = item.Id })</td>
            <td></td>
        </tr>

    }
</table>